{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "files": [ "app/AppKernel.php" ]
    },
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "2.8.*",
        "doctrine/orm": "^2.4.8",
        "doctrine/doctrine-bundle": "~1.6",
        "doctrine/doctrine-fixtures-bundle": "~2.2",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~5.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "incenteev/composer-parameter-handler": "~2.0",
        "knplabs/knp-paginator-bundle": "2.5.*",
        "knplabs/knp-menu-bundle": "2.1.*",
        "friendsofsymfony/user-bundle": "~1.3",
        "friendsofsymfony/comment-bundle": "~2.0",
        "instaclick/base-test-bundle": "dev-master",
        "psr/log": "~1.0",
        "twig/extensions": "~1.0",
        "sonata-project/admin-bundle": "~3.",
        "sonata-project/block-bundle": "^3.0",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/doctrine-extensions": "~1",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/user-bundle": "3.0.0",
        "sonata-project/jquery-bundle": "1.8.*",
        "sonata-project/exporter": "^1.4",
        "sonata-project/cache-bundle": "~2.2",
        "sonata-project/doctrine-orm-admin-bundle": "~3.0",
        "sonata-project/media-bundle": "~2.2",
        "pimple/pimple": "~1"

    },
    "require-dev": {
        "sensio/generator-bundle": "~2.2",
        "symfony/phpunit-bridge": "~2.7"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.8-dev"
        }
    }
}
